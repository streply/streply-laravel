{
    "name": "streply/streply-laravel",
    "description": "Streply Laravel SDK",
    "homepage": "https://streply.com",
    "type": "library",
    "keywords": ["streply", "php", "laravel", "log", "logging", "error-monitoring", "error-handler"],
    "license": "MIT",
    "authors": [
        {
            "name": "Streply",
            "email": "support@streply.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Streply\\Laravel\\": "src/"
        }
    },
    "require": {
        "php": "^8.2 || ^8.3 || ^8.4",
        "laravel/framework": "^10.0 || ^11.0 || ^12.0",
        "streply/streply-php": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Streply\\Laravel\\ServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
